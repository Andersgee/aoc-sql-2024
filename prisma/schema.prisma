// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["typedSql"]
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model day01 {
  id    Int @id @default(autoincrement())
  left  Int
  right Int
}

model day01test {
  id    Int @id @default(autoincrement())
  left  Int
  right Int
}

model day02 {
  id       Int            @id
  elements day02element[]
}

model day02element {
  index Int
  value Int

  day02Id   Int
  day02test day02 @relation(fields: [day02Id], references: [id], onDelete: Cascade)

  @@id([day02Id, index])
}
